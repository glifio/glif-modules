{
  "presets": ["@babel/preset-env", "@babel/preset-typescript"],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-runtime"
  ],
  "env": {
    "browser": {
      "presets": [
        [
          "@babel/env",
          {
            "targets": {
              "browsers": "last 2 versions, ie 10-11"
            },
            "modules": false
          }
        ]
      ]
    },
    "module": {
      "presets": [
        [
          "@babel/env",
          {
            "targets": {
              "node": "6.10"
            },
            "modules": false
          }
        ]
      ]
    }
  }
}
